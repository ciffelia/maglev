{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/.tsbuildinfo",

    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],

    "types": ["vite/client"],

    "module": "Preserve",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,

    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}
