{
  "name": "matrep",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --build && vite build",
    "preview": "pnpm run build && vite preview",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "migration:generate": "drizzle-kit generate",
    "migration:apply:local": "wrangler d1 migrations apply matrep-prod --local",
    "migration:apply:remote": "wrangler d1 migrations apply matrep-prod --remote",
    "typecheck": "tsc --build --noEmit",
    "lint": "pnpm run lint:eslint && pnpm run lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "pnpm run fix:eslint && pnpm run fix:prettier",
    "fix:eslint": "eslint . --fix",
    "fix:prettier": "prettier --write ."
  },
  "dependencies": {
    "drizzle-orm": "^0.40.1",
    "hono": "^4.7.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^0.1.12",
    "@cloudflare/workers-types": "^4.20250317.0",
    "@eslint/js": "^9.17.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.17.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-unicorn": "^57.0.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "wrangler": "^4.1.0"
  },
  "packageManager": "pnpm@10.6.4"
}
